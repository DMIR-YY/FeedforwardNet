name: "LeNet_2conv2max"
layer {
  name: "input"
  type: "Data"
  top: "input"
  top: "label"
  include {
    phase: TRAIN
  }
  transform_param {
    scale: 0.00390625
  }
  data_param {
    source: "/home/dmir/work/caffe/examples/mnist/mnist_train_lmdb"
    batch_size: 64
    backend: LMDB
  }
}
layer {
  name: "input"
  type: "Data"
  top: "input"
  top: "label"
  include {
    phase: TEST
  }
  transform_param {
    scale: 0.00390625
  }
  data_param {
    source: "/home/dmir/work/caffe/examples/mnist/mnist_test_lmdb"
    batch_size: 100
    backend: LMDB
  }
}


layer {
  name: "c1"
  type: "Convolution"
  bottom: "input"
  top: "c1"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  convolution_param {
    num_output: 6
    kernel_size: 5
    stride: 1
    pad: 2
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}

layer {
  name: "tan_c1"
  bottom: "c1"
  top: "tan_c1"
  type: "TanH"
}

# pool1
layer {
  name: "s2"
  type: "Pooling"
  bottom: "tan_c1"
  top: "s2"
  pooling_param {
    pool: MAX
    kernel_size: 2
    stride: 2
  }
}
layer {
  name: "tan_s2"
  bottom: "s2"
  top: "tan_s2"
  type: "TanH"
}
layer {
  name: "c3"
  type: "Convolution"
  bottom: "tan_s2"
  top: "c3"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  convolution_param {
    num_output: 16
    kernel_size: 5
    stride: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "tan_c3"
  bottom: "c3"
  top: "tan_c3"
  type: "TanH"
}
layer {
  name: "s4"
  type: "Pooling"
  bottom: "tan_c3"
  top: "s4"
  pooling_param {
    pool: MAX
    kernel_size: 2
    stride: 2
  }
}

layer {
  name: "tan_s4"
  bottom: "s4"
  top: "tan_s4"
  type: "TanH"
}


layer {
  name: "f5"
  type: "InnerProduct"
  bottom: "tan_s4"
  top: "f5"
  param {
    lr_mult:1
  }
  param {
    lr_mult:2
  }
  inner_product_param {
    num_output: 10
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "tan_f5"
  bottom: "f5"
  top: "tan_f5"
  type: "TanH"
}


#accuracy
layer {
  name: "accuracy"
  type: "Accuracy"
  bottom: "tan_f5"
  bottom: "label"
  top: "accuracy"
  include {
    phase: TEST
  }
}

#loss
layer {
  name: "loss"
  type: "SoftmaxWithLoss"
  bottom: "tan_f5"
  bottom: "label"
  top: "loss"
}
